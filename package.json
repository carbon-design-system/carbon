    },
    {
      "name": "Akira Sudoh",
      "email": "asudoh@jp.ibm.com"
    },
    {
      "name": "Chris Dhanaraj",
      "email": "chrisdhanaraj@us.ibm.com"
    }
  ],
  "eyeglass": {
    "sassDir": "scss",
    "exports": false,
    "name": "carbon-components",
    "needs": "^1.2.1"
  }
}
