    "minimist": "~1.2.0",
    "mocha": "~2.4.4",
    "mock-raf": "^1.0.0",
    "nodemon": "1.9.1",
    "prettier": "^1.7.0",
    "pump": "^1.0.2",
    "rollup": "^0.49.0",
    "rollup-plugin-babel": "^3.0.0",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-string": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.0",
    "scss-to-json": "1.1.0",
