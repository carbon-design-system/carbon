    },
    {
      "name": "Akira Sudoh",
      "email": "asudoh@jp.ibm.com"
    },
    {
      "name": "Alison Joseph",
      "email": "alison.joseph@us.ibm.com"
    },
    {
      "name": "Alessandra Davila",
      "email": "aledavila@us.ibm.com"
    }
  ],
  "eyeglass": {
    "sassDir": "scss",
    "exports": false,
    "name": "carbon-components",
    "needs": "^1.2.1"
  }
}
