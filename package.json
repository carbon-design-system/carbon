{
  "name": "@console/bluemix-components-react",
  "version": "1.6.2",
  "description": "A React wrapper for bluemix-components",
  "main": "dist/index.js",
  "scripts": {
    "check": "npm run lint && npm run test",
    "coverage": "jest '/(components|lib|internal)/' --coverage && node scripts/create-badge.js",
    "lint": "eslint --ignore-pattern dist .",
    "storybook": "start-storybook -p 9001",
    "test": "jest '/(components|lib|internal)/'",
    "test-watch": "jest '/(components|lib|internal)/' --watch",
    "test-a11y": "jest '/a11y/'",
    "build-storybook": "build-storybook -s .storybook -o .gh-pages/storybook",
    "gh-pages": "node scripts/gh-pages.js",
    "build-es5": "babel . -d dist --ignore a11y,__tests__,node_modules,scripts,deploy.js",
    "commitmsg": "validate-commit-msg",
    "commit": "git cz"
  },
  "keywords": [
    "react",
    "bluemix",
    "bluemix-components"
  ],
  "files": [
    "dist/**/*"
  ],
  "contributors": [
    {
      "name": "Brian Han",
      "email": "bthan@us.ibm.com"
    },
    {
      "name": "Aziz Punjani",
      "email": "punjani@us.ibm.com"
    },
    {
      "name": "Mari Johannessen",
      "email": "mjohannessen@us.ibm.com"
    },
    {
      "name": "T.J. Eagan",
      "email": "twegan@us.ibm.com"
    },
    {
      "name": "Ian Fleming",
      "email": "ianfleming@us.ibm.com"
    },
    {
      "name": "Chris Dhanaraj",
      "email": "chrisdhanaraj@us.ibm.com"
    }
  ],
  "license": "MIT",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "arrow-parens": 0,
      "comma-dangle": 0,
      "max-len": [
        2,
        130,
        4
      ],
      "no-underscore-dangle": 0,
      "global-require": 0,
      "no-mixed-operators": 0,
      "no-plusplus": 0,
      "no-prototype-builtins": 0,
      "no-useless-return": 0,
      "semi-spacing": 0,
      "space-unary-ops": 0,
      "import/first": 0,
      "import/newline-after-import": 0,
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true,
          "optionalDependencies": false,
          "peerDependencies": true
        }
      ],
      "import/prefer-default-export": 0,
      "jsx-a11y/href-no-hash": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/tabindex-no-positive": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-indent": 0,
      "react/jsx-wrap-multilines": 0,
      "react/no-find-dom-node": 0,
      "react/no-string-refs": 0,
      "react/prefer-stateless-function": 0,
      "react/self-closing-comp": 0
    },
    "env": {
      "jest": true,
      "jasmine": true
    }
  },
  "dependencies": {
    "@console/bluemix-components": "6.14.2",
    "@console/bluemix-icons": "3.6.0",
    "classnames": "2.2.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "warning": "3.0.0",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "@ibma/aat": "^1.0.29",
    "@kadira/react-storybook-addon-info": "3.2.1",
    "@kadira/react-storybook-decorator-centered": "^1.0.0",
    "@kadira/storybook": "2.20.0",
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-dynamic-import-node": "^1.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.16.0",
    "commitizen": "^2.9.5",
    "css-loader": "0.23.1",
    "cz-conventional-changelog-components": "^1.0.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "gh-pages": "0.11.0",
    "husky": "^0.13.1",
    "jest": "^18.1.0",
    "lcov2badge": "^0.1.0",
    "node-sass": "3.8.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.1",
    "sass-loader": "4.0.0",
    "shelljs": "^0.7.5",
    "style-loader": "0.13.1",
    "validate-commit-msg": "^2.10.1"
  },
  "jest": {
    "coverageDirectory": "./.gh-pages/coverage",
    "moduleNameMapper": {
      "^.+\\.(scss)$": "<rootDir>/lib/styleMock.js"
    }
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "Subject does not match subject pattern!"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-components"
    }
  }
}
