    "gulp-uglify": "^2.1.2",
    "gulp-util": "~3.0.7",
    "husky": "^0.12.0",
    "karma": "^1.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-html2js-preprocessor": "^1.1.0",
