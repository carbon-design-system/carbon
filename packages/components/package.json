  "homepage": "https://www.carbondesignsystem.com/",
  "engines": {
    "node": ">=6.x"
  },
  "files": [
    "css/**/*",
    "es/**/*",
    "html/**/*",
    "scripts/**/*",
    "scss/**/*",
    "src/**/*",
    "umd/**/*",
    "!**/*.config.js",
    "!**/__tests__/**/*"
  ],
  "keywords": [
    "carbon",
    "component",
    "components",
    "css",
    "html",
    "ibm",
    "javascript",
    "js",
    "library",
    "pattern",
    "patterns",
