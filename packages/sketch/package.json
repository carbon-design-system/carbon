{
  "name": "@carbon/sketch",
  "private": true,
  "description": "Tooling for generating a sketch plugin to bring code to design",
  "version": "11.19.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/carbon.git",
    "directory": "packages/sketch"
  },
  "bugs": "https://github.com/carbon-design-system/carbon/issues",
  "keywords": [
    "ibm",
    "carbon",
    "carbon-design-system",
    "components",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:plugin": "node tasks/build.js && cross-env NODE_ENV=production skpm-build",
    "clean": "rimraf carbon-elements.sketchplugin generated",
    "develop": "cross-env NODE_ENV=development skpm-build --watch",
    "log": "skpm log -f",
    "skpm:link": "skpm-link"
  },
  "dependencies": {
    "@carbon/colors": "^11.12.0",
    "@carbon/icon-helpers": "^10.38.0",
    "@carbon/icons": "^11.16.1",
    "@carbon/themes": "^11.16.0",
    "@carbon/type": "^11.15.0",
    "@skpm/builder": "^0.8.0",
    "color-string": "^1.5.3",
    "core-js": "^3.16.0",
    "regenerator-runtime": "^0.13.7",
    "skpm": "^1.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "rimraf": "^4.0.0"
  },
  "skpm": {
    "name": "Carbon Elements",
    "manifest": "src/manifest.json",
    "main": "carbon-elements.sketchplugin",
    "assets": []
  }
}
