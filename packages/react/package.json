    },
    {
      "name": "TJ Egan",
      "email": "twegan@us.ibm.com"
    },
    {
      "name": "Ian Fleming",
      "email": "ianfleming@us.ibm.com"
    },
    {
      "name": "Chris Dhanaraj",
      "email": "chrisdhanaraj@us.ibm.com"
    }
  ],
  "bundleSizeThreshold": 120000,
  "release": {
    "branch": "master"
  }
}
