{
  "name": "@rocketsoftware/type",
  "description": "Typography for digital and software products using the Carbon Design System",
  "version": "0.0.8",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "https://github.com/RocketSoftware/carbon/tree/master/packages/type",
  "bugs": "https://github.com/RocketSoftware/carbon/issues",
  "files": [
    "es",
    "lib",
    "scss",
    "src",
    "umd"
  ],
  "keywords": [
    "eyeglass-module",
    "rocketsoftware",
    "elements",
    "carbon",
    "carbon-elements",
    "carbon-design-system",
    "components",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && bundler bundle src/index.js --name CarbonType && bundler sassdoc \"scss/**/*.scss\" -i \"**/{_inlined,vendor}**\" && bundler inline && bundler check \"scss/*.scss\"",
    "clean": "rimraf css es lib umd scss/_inlined scss/vendor"
  },
  "dependencies": {
    "@carbon/import-once": "10.3.0",
    "@rocketsoftware/layout": "^0.0.8"
  },
  "devDependencies": {
    "@carbon/bundler": "10.4.0",
    "@carbon/test-utils": "10.4.0-rc.0",
    "change-case": "^3.1.0",
    "rimraf": "^2.6.2"
  },
  "eyeglass": {
    "exports": false,
    "name": "@rocketsoftware/type",
    "sassDir": "scss",
    "needs": "^1.3.0"
  }
}
