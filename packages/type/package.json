    "postinstall": "ibmtelemetry --config=telemetry.yml"
  },
  "dependencies": {
    "@carbon/grid": "^10.43.3",
    "@carbon/import-once": "^10.7.1",
    "@ibm/telemetry-js": "^1.2.1"
  },
  "devDependencies": {
    "@carbon/cli": "^10.34.3",
    "@carbon/test-utils": "^10.21.1",
    "change-case": "^4.1.1",
    "rimraf": "^3.0.0"
  },
  "eyeglass": {
    "exports": false,
    "name": "@carbon/type",
    "sassDir": "scss",
    "needs": "^1.3.0"
  }
}
